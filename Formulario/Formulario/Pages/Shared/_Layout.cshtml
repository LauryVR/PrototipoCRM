<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Formulario</title>


    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link href="css/navbar.css" rel="stylesheet">
    
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/PlanesEstudio.css" />

</head>
<body>
    <header>
        <div class="navbar navbar-expand-md card-body text-center" role="navigation" style="background-color:brown; color:white">
            <a class="navbar-expand-lg" href="#" >
                <img src="https://www.bi-dss.com/wp-content/uploads/2019/06/favicon.png" href="Index" alt="logo" style="width:70px;" >
            </a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation" style="background-color:brown; color:white;">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <ul class="navbar-nav mr-auto" style="color:white">
                        <li class="nav-item active" >
                        <a class="nav-link" href="Index" style="color:white;">Inicio</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="Cursos" style="color:white;">Cursos</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="carreras" style="color:white;">Planes De Estudio</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="Seguimiento" style="color:white;">Seguimiento</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="Registro" style="color:white;">Registro</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="Historial" style="color:white;">Historial</a>
                    </li>

                </ul>

            </div>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @*<footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Formulario - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>*@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
